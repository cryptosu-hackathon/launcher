{
	"name": "cryptosu-launcher",
	"version": "0.1.0",
	"description": "Launcher for the cryptOsu project, providing wallet relay & API.",
	"private": true,
	"scripts": {
		"clean": "rm -rf ./dist/",
		"build": "npx webpack",
		"watch": "npx webpack --watch",
		"lint": "eslint -c .eslintrc --ext .ts ./src",
		"start": "npm run build && npx electron ./dist/main.js",
		"electron": "npx electron ./dist/main.js"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.29.2",
		"@typescript-eslint/parser": "^4.29.2",
		"electron": "^14.0.1",
		"eslint": "^7.32.0",
		"html-webpack-plugin": "^5.3.2",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.3",
		"webpack": "^5.53.0",
		"webpack-cli": "^4.8.0"
	}
}
